<div id="newMovie" class="jumbotron bg-grey">
<form class="form-horizontal" id="newMovieForm">
  <fieldset>
    <legend><i class="fa fa-users" aria-hidden="true"></i> Manage Movies</legend>
    <h3>Movies </h3> 
    <% for (let i=0; i<movies.length; i++) {%>
<div class="col-md-8 screen-timings st">
            <div class="col-md-4">
            <h3><a href="/admin/edit-movie/1" > <%= movies[i].name %> </a></h3>
            </div>
            <div class="col-md-6">
                <a class="btn btn-primary btn-md" data-id="" id=""   role="button" ><i class="fa fa-edit" aria-hidden="true"></i> Edit</a>
                <a class="btn btn-danger btn-md" data-id="" id="delete-id"  role="button"><i class="fa fa-close" aria-hidden="true"></i> Delete</a>
            </div>
        </div>
        <% } %>
    </fieldset>
    </form>
    </div>